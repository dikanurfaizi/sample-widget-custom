  "scripts": {
    "start": "node react-ts -- serve 8080",
    "build": "node react-ts -- build",
    "build:dev": "node react-ts -- build development",
    "lint": "tslint src/**/*.tsx",
    "tdd": "concurrently -r 'npm run watch' 'npm run test:watch'",
    "test": "npm run compile && istanbul cover ./node_modules/mocha/bin/_mocha -- dist/**/*.spec.js",
    "test:watch": "nodemon --delay 1 --watch dist --exec 'istanbul cover ./node_modules/mocha/bin/_mocha -- dist/**/*.spec.js'",
    "compile": "tsc",
    "watch": "npm run compile -- -w",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "pack:cli": "rimraf build dist && cross-env NODE_ENV=production webpack --config react-ts/webpack.bundle.js",
    "pack-custom:cli": "rimraf build dist && cross-env NODE_ENV=dev webpack --config react-ts/webpack.bundle.dev.js",
    "pack": "rimraf build dist && cross-env NODE_ENV=production webpack --config react-ts/webpack.bundle.js --json"
  },
