    "bd-statuscode": "file:dependency/bd-statuscode-0.1.27.tgz",
    "chai": "^4.1.2",
    "dts-bundle-webpack": "^1.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.10.0",
    "typedoc": "^0.16.10",
